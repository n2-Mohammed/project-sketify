<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/main"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="#FFFFFF"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/linear_btm"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical">
			<ScrollView
				android:id="@+id/vscroll1"
				android:layout_width="match_parent"
				android:layout_height="match_parent">
				<LinearLayout
					android:id="@+id/linear5"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:padding="4dp"
					android:background="#FFFFFF"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/linear54"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="vertical">
						<LinearLayout
							android:id="@+id/linear1"
							android:layout_width="match_parent"
							android:layout_height="180dp"
							android:layout_marginTop="10dp"
							android:layout_marginBottom="10dp"
							android:paddingLeft="8dp"
							android:paddingTop="8dp"
							android:paddingRight="8dp"
							android:background="#EEEEEE"
							android:gravity="left|bottom"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/profilepic"
								android:layout_width="100dp"
								android:layout_height="100dp"
								android:layout_marginLeft="2dp"
								android:paddingTop="10dp"
								android:paddingRight="16dp"
								android:paddingBottom="10dp"
								android:src="@drawable/default_avatar"
								android:scaleType="fitCenter"
								android:layout_gravity="left" />
							<LinearLayout
								android:id="@+id/linear3"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:layout_marginBottom="10dp"
								android:gravity="bottom"
								android:orientation="vertical">
								<TextView
									android:id="@+id/name"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:paddingLeft="10dp"
									android:paddingTop="4dp"
									android:paddingRight="4dp"
									android:paddingBottom="4dp"
									android:text="Username"
									android:textSize="20sp"
									android:textStyle="bold"
									android:textColor="#536DFE" />
								<LinearLayout
									android:id="@+id/linear4"
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:paddingLeft="4dp"
									android:orientation="horizontal">
									<TextView
										android:id="@+id/email"
										android:layout_width="wrap_content"
										android:layout_height="match_parent"
										android:paddingLeft="6dp"
										android:gravity="center_vertical"
										android:text="user\@mail.com"
										android:textSize="12sp"
										android:textColor="#3D5AFE" />
								</LinearLayout>
							</LinearLayout>
						</LinearLayout>
						<LinearLayout
							android:id="@+id/profile"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:background="#FFFFFF"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/img1"
								android:layout_width="50dp"
								android:layout_height="match_parent"
								android:padding="5dp"
								android:src="@drawable/ic_person_black"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/textview6"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Profile"
								android:textSize="12sp"
								android:textColor="#536DFE" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/pricings"
							android:layout_width="match_parent"
							android:layout_height="50dp"
							android:layout_marginTop="10dp"
							android:layout_marginBottom="10dp"
							android:padding="8dp"
							android:background="#FFFFFF"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/img2"
								android:layout_width="50dp"
								android:layout_height="match_parent"
								android:padding="5dp"
								android:src="@drawable/dollar"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/textview31"
								android:layout_width="wrap_content"
								android:layout_height="match_parent"
								android:padding="8dp"
								android:gravity="center_vertical"
								android:text="Pricings"
								android:textSize="12sp"
								android:textColor="#536DFE" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/p_manage"
							android:layout_width="match_parent"
							android:layout_height="50dp"
							android:layout_marginBottom="4dp"
							android:padding="8dp"
							android:background="#FFFFFF"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/img3"
								android:layout_width="50dp"
								android:layout_height="match_parent"
								android:padding="8dp"
								android:src="@drawable/file_80px"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/textview9"
								android:layout_width="wrap_content"
								android:layout_height="match_parent"
								android:padding="8dp"
								android:gravity="center_vertical"
								android:text="SW Project Manager"
								android:textSize="12sp"
								android:textColor="#536DFE" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear58"
							android:layout_width="match_parent"
							android:layout_height="1dp"
							android:layout_marginLeft="10dp"
							android:layout_marginTop="8dp"
							android:layout_marginRight="10dp"
							android:padding="8dp"
							android:background="#3D5AFE"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/codes"
							android:layout_width="match_parent"
							android:layout_height="50dp"
							android:layout_marginTop="10dp"
							android:padding="8dp"
							android:background="#FFFFFF"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/img4"
								android:layout_width="50dp"
								android:layout_height="match_parent"
								android:src="@drawable/codeview"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/textview5"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Java Codes"
								android:textSize="12sp"
								android:textColor="#536DFE" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/share"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginTop="10dp"
							android:layout_marginBottom="4dp"
							android:padding="8dp"
							android:background="#FFFFFF"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/img5"
								android:layout_width="50dp"
								android:layout_height="match_parent"
								android:padding="5dp"
								android:src="@drawable/ic_share_black"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/textview30"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Share"
								android:textSize="12sp"
								android:textColor="#536DFE" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear61"
							android:layout_width="match_parent"
							android:layout_height="1dp"
							android:layout_marginLeft="8dp"
							android:layout_marginTop="8dp"
							android:layout_marginRight="8dp"
							android:padding="8dp"
							android:background="#3D5AFE"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/community"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginTop="10dp"
							android:padding="8dp"
							android:background="#FFFFFF"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/img6"
								android:layout_width="50dp"
								android:layout_height="match_parent"
								android:padding="6dp"
								android:src="@drawable/ic_favorite_black"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/textview3"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Liked Projects"
								android:textSize="12sp"
								android:textColor="#536DFE" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/blog"
							android:layout_width="match_parent"
							android:layout_height="50dp"
							android:layout_marginTop="10dp"
							android:padding="8dp"
							android:background="#FFFFFF"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/img7"
								android:layout_width="50dp"
								android:layout_height="match_parent"
								android:padding="5dp"
								android:src="@drawable/ic_open_in_new_black"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/textview8"
								android:layout_width="wrap_content"
								android:layout_height="match_parent"
								android:padding="8dp"
								android:gravity="center_vertical"
								android:text="Blog Posts"
								android:textSize="12sp"
								android:textColor="#536DFE" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear60"
							android:layout_width="match_parent"
							android:layout_height="1dp"
							android:layout_marginTop="8dp"
							android:padding="8dp"
							android:background="#3D5AFE"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/discord"
							android:layout_width="match_parent"
							android:layout_height="50dp"
							android:layout_marginTop="10dp"
							android:layout_marginBottom="4dp"
							android:padding="8dp"
							android:background="#FFFFFF"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/img8"
								android:layout_width="50dp"
								android:layout_height="match_parent"
								android:padding="3dp"
								android:src="@drawable/discordd"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/textview33"
								android:layout_width="wrap_content"
								android:layout_height="match_parent"
								android:padding="8dp"
								android:gravity="center_vertical"
								android:text="Join Us On Discord"
								android:textSize="12sp"
								android:textColor="#536DFE" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/rateus"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginTop="10dp"
							android:padding="8dp"
							android:background="#FFFFFF"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/img11"
								android:layout_width="50dp"
								android:layout_height="match_parent"
								android:padding="5dp"
								android:src="@drawable/ic_chat_black"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/textview34"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Rate Us"
								android:textSize="12sp"
								android:textColor="#536DFE" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/settings"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginTop="10dp"
							android:padding="8dp"
							android:background="#FFFFFF"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/img9"
								android:layout_width="50dp"
								android:layout_height="match_parent"
								android:padding="5dp"
								android:src="@drawable/ic_settings_black"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/textview7"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Settings"
								android:textSize="12sp"
								android:textColor="#536DFE" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/about_us"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginTop="10dp"
							android:padding="8dp"
							android:background="#FFFFFF"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/img10"
								android:layout_width="50dp"
								android:layout_height="match_parent"
								android:padding="6dp"
								android:src="@drawable/ic_info_black"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/textview29"
								android:layout_width="wrap_content"
								android:layout_height="match_parent"
								android:padding="8dp"
								android:gravity="center_vertical"
								android:text="About Us"
								android:textSize="12sp"
								android:textColor="#536DFE" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear53"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginTop="10dp"
							android:padding="8dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/web_logo"
								android:layout_width="45dp"
								android:layout_height="45dp"
								android:paddingLeft="10dp"
								android:paddingTop="9dp"
								android:paddingRight="10dp"
								android:paddingBottom="9dp"
								android:background="#FFFFFF"
								android:src="@drawable/ic_vpn_lock_black"
								android:scaleType="fitCenter" />
							<ImageView
								android:id="@+id/twitter_logo"
								android:layout_width="45dp"
								android:layout_height="45dp"
								android:layout_marginLeft="10dp"
								android:layout_marginRight="10dp"
								android:src="@drawable/img_vects_3"
								android:scaleType="centerInside" />
							<ImageView
								android:id="@+id/yt_logo"
								android:layout_width="45dp"
								android:layout_height="45dp"
								android:src="@drawable/img_vects_4"
								android:scaleType="centerInside" />
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</ScrollView>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
