					android:textSize="16sp"
					android:textColor="#FFFFFF"
					android:singleLine="true"
					android:layout_weight="1" />
				<ImageView
					android:id="@+id/imageview23"
					android:layout_width="40dp"
					android:layout_height="40dp"
					android:padding="10dp"
