<androidx.coordinatorlayout.widget.CoordinatorLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/_coordinator"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/main"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:background="#FFFFFF"
			android:orientation="horizontal"
			android:layout_weight="1">
			<ScrollView
				android:id="@+id/vscroll1"
				android:layout_width="match_parent"
				android:layout_height="match_parent">
				<LinearLayout
					android:id="@+id/linear1"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/top"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal">
						<LinearLayout
							android:id="@+id/base"
							android:layout_width="match_parent"
							android:layout_height="290dp"
							android:orientation="vertical"
							android:layout_weight="1">
							<LinearLayout
								android:id="@+id/depart"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:background="#536DFE"
								android:gravity="center_horizontal|bottom"
								android:orientation="vertical">
								<LinearLayout
									android:id="@+id/ic_linear"
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:layout_marginTop="8dp"
									android:gravity="center_horizontal|bottom"
									android:orientation="horizontal">
									<LinearLayout
										android:id="@+id/linear42"
										android:layout_width="match_parent"
										android:layout_height="match_parent"
										android:padding="8dp"
										android:orientation="horizontal"
										android:layout_weight="1" />
									<ImageView
										android:id="@+id/imageview1"
										android:layout_width="100dp"
										android:layout_height="100dp"
										android:layout_marginBottom="8dp"
										android:src="@drawable/default_avatar"
										android:scaleType="fitCenter" />
									<LinearLayout
										android:id="@+id/linear43"
										android:layout_width="match_parent"
										android:layout_height="match_parent"
										android:padding="8dp"
										android:gravity="right|top"
										android:orientation="horizontal"
										android:layout_weight="1">
										<Button
											android:id="@+id/ban_btn"
											android:layout_width="wrap_content"
											android:layout_height="50dp"
											android:padding="8dp"
											android:background="#F44336"
											android:text="BAN"
											android:textSize="16sp"
											android:textStyle="bold"
											android:textColor="#FFFFFF"
											android:layout_gravity="right|top" />
									</LinearLayout>
								</LinearLayout>
								<TextView
									android:id="@+id/textview_username"
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:layout_marginBottom="8dp"
									android:gravity="center_horizontal|center_vertical"
									android:text="Company"
									android:textSize="18sp"
									android:textStyle="bold"
									android:textColor="#FFFFFF" />
								<LinearLayout
									android:id="@+id/linear38"
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:layout_marginBottom="50dp"
									android:gravity="center_horizontal|center_vertical"
									android:orientation="horizontal">
									<HorizontalScrollView
										android:id="@+id/hscroll1"
										android:layout_width="wrap_content"
										android:layout_height="wrap_content"
										android:layout_gravity="center_horizontal|center_vertical">
										<LinearLayout
											android:id="@+id/linear33"
											android:layout_width="wrap_content"
											android:layout_height="wrap_content"
											android:gravity="center_horizontal|center_vertical"
											android:orientation="horizontal">
											<LinearLayout
												android:id="@+id/vip"
												android:layout_width="wrap_content"
												android:layout_height="wrap_content"
												android:orientation="horizontal"
												android:layout_weight="1">
												<ImageView
													android:id="@+id/vip_ic"
													android:layout_width="30dp"
													android:layout_height="match_parent"
													android:paddingLeft="2dp"
													android:paddingTop="2dp"
													android:src="@drawable/badges_2"
													android:scaleType="fitCenter" />
												<TextView
													android:id="@+id/textview21"
													android:layout_width="wrap_content"
													android:layout_height="wrap_content"
													android:paddingLeft="4dp"
													android:paddingTop="8dp"
													android:paddingRight="8dp"
													android:paddingBottom="8dp"
													android:text="VIP"
													android:textSize="12sp"
													android:textStyle="bold"
													android:textColor="#FFFFFF" />
											</LinearLayout>
											<LinearLayout
												android:id="@+id/developer"
												android:layout_width="wrap_content"
												android:layout_height="wrap_content"
												android:orientation="horizontal"
												android:layout_weight="1">
												<ImageView
													android:id="@+id/developer_ic"
													android:layout_width="30dp"
													android:layout_height="match_parent"
													android:layout_marginLeft="2dp"
													android:padding="7dp"
													android:src="@drawable/ic_developer_mode_black"
													android:scaleType="fitCenter" />
												<TextView
													android:id="@+id/textview20"
													android:layout_width="wrap_content"
													android:layout_height="wrap_content"
													android:paddingTop="8dp"
													android:paddingRight="8dp"
													android:paddingBottom="8dp"
													android:text="Developer"
													android:textSize="12sp"
													android:textStyle="bold"
													android:textColor="#FFFFFF" />
											</LinearLayout>
											<LinearLayout
												android:id="@+id/premium"
												android:layout_width="wrap_content"
												android:layout_height="wrap_content"
												android:orientation="horizontal"
												android:layout_weight="1">
												<ImageView
													android:id="@+id/premium_ic"
													android:layout_width="30dp"
													android:layout_height="match_parent"
													android:layout_marginLeft="3dp"
													android:paddingBottom="2dp"
													android:src="@drawable/badges_5"
													android:scaleType="fitCenter" />
												<TextView
													android:id="@+id/category"
													android:layout_width="wrap_content"
													android:layout_height="wrap_content"
													android:paddingTop="8dp"
													android:paddingRight="8dp"
													android:paddingBottom="8dp"
													android:text="Premium"
													android:textSize="12sp"
													android:textStyle="bold"
													android:textColor="#FFFFFF" />
											</LinearLayout>
											<LinearLayout
												android:id="@+id/verified_linear"
												android:layout_width="wrap_content"
												android:layout_height="wrap_content"
												android:orientation="horizontal"
												android:layout_weight="1">
												<ImageView
													android:id="@+id/verified_ic"
													android:layout_width="30dp"
													android:layout_height="match_parent"
													android:paddingLeft="4dp"
													android:paddingTop="2dp"
													android:src="@drawable/badges_7"
													android:scaleType="fitCenter" />
												<TextView
													android:id="@+id/textview22"
													android:layout_width="wrap_content"
													android:layout_height="wrap_content"
													android:paddingLeft="4dp"
													android:paddingTop="8dp"
													android:paddingRight="8dp"
													android:paddingBottom="8dp"
													android:text="Verified"
													android:textSize="12sp"
													android:textStyle="bold"
													android:textColor="#FFFFFF" />
											</LinearLayout>
											<LinearLayout
												android:id="@+id/admin"
												android:layout_width="wrap_content"
												android:layout_height="wrap_content"
												android:orientation="horizontal"
												android:layout_weight="1">
												<ImageView
													android:id="@+id/admin_ic"
													android:layout_width="30dp"
													android:layout_height="match_parent"
													android:paddingLeft="4dp"
													android:src="@drawable/badges_8"
													android:scaleType="fitCenter" />
												<TextView
													android:id="@+id/textview25"
													android:layout_width="wrap_content"
													android:layout_height="wrap_content"
													android:padding="8dp"
													android:text="Admin"
													android:textSize="12sp"
													android:textStyle="bold"
													android:textColor="#FFFFFF" />
											</LinearLayout>
											<LinearLayout
												android:id="@+id/moderator"
												android:layout_width="wrap_content"
												android:layout_height="wrap_content"
												android:orientation="horizontal"
												android:layout_weight="1">
												<ImageView
													android:id="@+id/moderator_ic"
													android:layout_width="32dp"
													android:layout_height="match_parent"
													android:paddingTop="1dp"
													android:src="@drawable/badges_3"
													android:scaleType="fitCenter" />
												<TextView
													android:id="@+id/textview23"
													android:layout_width="wrap_content"
													android:layout_height="wrap_content"
													android:paddingTop="8dp"
													android:paddingRight="8dp"
													android:paddingBottom="8dp"
													android:text="Moderator"
													android:textSize="12sp"
													android:textStyle="bold"
													android:textColor="#FFFFFF" />
											</LinearLayout>
										</LinearLayout>
									</HorizontalScrollView>
								</LinearLayout>
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear34"
								android:layout_width="match_parent"
								android:layout_height="80dp"
								android:layout_marginLeft="20dp"
								android:layout_marginRight="20dp"
								android:padding="8dp"
								android:background="#FFFFFF"
								android:orientation="horizontal"
								android:translationY="-45.0dp">
								<LinearLayout
									android:id="@+id/linear35"
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									android:padding="8dp"
									android:orientation="vertical"
									android:layout_weight="1">
									<TextView
										android:id="@+id/textview26"
										android:layout_width="match_parent"
										android:layout_height="wrap_content"
										android:paddingLeft="8dp"
										android:paddingTop="4dp"
										android:paddingRight="8dp"
										android:paddingBottom="4dp"
										android:gravity="center_horizontal"
										android:text="Projects"
										android:textSize="12sp"
										android:textColor="#000000"
										android:alpha="0.5" />
									<TextView
										android:id="@+id/textview_projects"
										android:layout_width="match_parent"
										android:layout_height="wrap_content"
										android:paddingTop="2dp"
										android:gravity="center_horizontal"
										android:text="0"
										android:textSize="12sp"
										android:textStyle="bold"
										android:textColor="#000000" />
								</LinearLayout>
								<LinearLayout
									android:id="@+id/linear39"
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									android:padding="8dp"
									android:orientation="vertical"
									android:layout_weight="1">
									<TextView
										android:id="@+id/textview33"
										android:layout_width="match_parent"
										android:layout_height="wrap_content"
										android:paddingLeft="8dp"
										android:paddingTop="4dp"
										android:paddingRight="8dp"
										android:paddingBottom="4dp"
										android:gravity="center_horizontal"
										android:text="Likes"
										android:textSize="12sp"
										android:textColor="#000000"
										android:alpha="0.5" />
									<TextView
										android:id="@+id/textview34"
										android:layout_width="match_parent"
										android:layout_height="wrap_content"
										android:paddingTop="2dp"
										android:gravity="center_horizontal"
										android:text="0"
										android:textSize="12sp"
										android:textStyle="bold"
										android:textColor="#3D5AFE" />
								</LinearLayout>
								<LinearLayout
									android:id="@+id/linear41"
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									android:padding="8dp"
									android:orientation="vertical"
									android:layout_weight="1">
									<TextView
										android:id="@+id/textview37"
										android:layout_width="match_parent"
										android:layout_height="wrap_content"
										android:paddingLeft="8dp"
										android:paddingTop="4dp"
										android:paddingRight="8dp"
										android:paddingBottom="4dp"
										android:gravity="center_horizontal"
										android:text="Downloads"
										android:textSize="12sp"
										android:textColor="#000000"
										android:alpha="0.5" />
									<TextView
										android:id="@+id/textview38"
										android:layout_width="match_parent"
										android:layout_height="wrap_content"
										android:paddingTop="2dp"
										android:gravity="center_horizontal"
										android:text="0"
										android:textSize="12sp"
										android:textStyle="bold"
										android:textColor="#000000" />
								</LinearLayout>
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/bottom"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:orientation="horizontal"
						android:layout_weight="1">
						<LinearLayout
							android:id="@+id/feed"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical"
							android:layout_weight="1">
							<TextView
								android:id="@+id/textview_bio"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:gravity="center_horizontal|center_vertical"
								android:text="Bio Detail"
								android:textSize="12sp"
								android:textColor="#000000" />
							<ListView
								android:id="@+id/listview1"
								android:layout_width="match_parent"
								android:layout_height="match_parent"
								android:padding="8dp"
								android:choiceMode="none"
								android:layout_weight="1" />
							<LinearLayout
								android:id="@+id/nothing"
								android:layout_width="match_parent"
								android:layout_height="match_parent"
								android:padding="8dp"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="vertical"
								android:layout_weight="1">
								<ImageView
									android:id="@+id/imageview2"
									android:layout_width="wrap_content"
									android:layout_height="150dp"
									android:src="@drawable/cb1_3"
									android:scaleType="centerInside" />
								<TextView
									android:id="@+id/textview31"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:padding="8dp"
									android:text="No Projects Shared Yet!"
									android:textSize="20sp"
									android:textStyle="bold"
									android:textColor="#536DFE" />
								<TextView
									android:id="@+id/textview32"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:padding="8dp"
									android:text="Click &quot;+&quot; Fab In Sketify Home To Share A Project Publicly."
									android:textSize="10sp"
									android:textColor="#000000" />
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</ScrollView>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/acc_banned"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal"
			android:layout_weight="1">
			<LinearLayout
				android:id="@+id/linear59"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:background="#536DFE"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/linear60"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:orientation="horizontal"
					android:layout_weight="1" />
				<LinearLayout
					android:id="@+id/linear61"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="4dp"
					android:background="#F44336"
					android:orientation="vertical"
					android:alpha="0.9">
					<TextView
						android:id="@+id/textview60"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:background="#F44336"
						android:text="ACCOUNT BANNED"
						android:textSize="16sp"
						android:textStyle="bold"
						android:textColor="#FFFFFF"
						android:alpha="0.9" />
				</LinearLayout>
				<TextView
					android:id="@+id/textview61"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginTop="10dp"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:text="This Account was Banned By Administrators."
					android:textSize="12sp"
					android:textColor="#FFFFFF" />
				<LinearLayout
					android:id="@+id/linear62"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:orientation="horizontal"
					android:layout_weight="1" />
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
	<com.google.android.material.floatingactionbutton.FloatingActionButton
		android:id="@+id/_fab"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_margin="16dp"
		android:layout_gravity="right|bottom"
		app:srcCompat="@drawable/ic_edit_white" />
</androidx.coordinatorlayout.widget.CoordinatorLayout>
